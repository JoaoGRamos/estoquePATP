program estoque;

uses
  Vcl.Forms,
  pasPrincipal in 'pasPrincipal.pas' {frmPrincipal},
  pasCAproduto in 'pasCAproduto.pas' {frmCAproduto},
  pasCategoria in 'pasCategoria.pas' {frmCategoria},
  pasSubcat in 'pasSubcat.pas' {frmSubcat},
  pasUmedida in 'pasUmedida.pas' {frmUmedida},
  pasCAcliente in 'pasCAcliente.pas' {frmCAcliente},
  pasCAfornecedor in 'pasCAfornecedor.pas' {frmCAfornecedor},
  pasCOproduto in 'pasCOproduto.pas' {frmCOproduto},
  pasCOcliente in 'pasCOcliente.pas' {frmCOcliente},
  pasCOfornecedor in 'pasCOfornecedor.pas' {frmCOfornecedor},
  pasVenda in 'pasVenda.pas' {frmVenda},
  pasCompra in 'pasCompra.pas' {frmCompra};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmPrincipal, frmPrincipal);
  Application.CreateForm(TfrmCAproduto, frmCAproduto);
  Application.CreateForm(TfrmCategoria, frmCategoria);
  Application.CreateForm(TfrmSubcat, frmSubcat);
  Application.CreateForm(TfrmUmedida, frmUmedida);
  Application.CreateForm(TfrmCAcliente, frmCAcliente);
  Application.CreateForm(TfrmCAfornecedor, frmCAfornecedor);
  Application.CreateForm(TfrmCOproduto, frmCOproduto);
  Application.CreateForm(TfrmCOcliente, frmCOcliente);
  Application.CreateForm(TfrmCOfornecedor, frmCOfornecedor);
  Application.CreateForm(TfrmVenda, frmVenda);
  Application.CreateForm(TfrmCompra, frmCompra);
  Application.Run;
end.
